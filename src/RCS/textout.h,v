    struct BITMAP *, struct FONT *, const char *, int x, int y, int color);
void textout_centre_trans_magic (
    struct BITMAP *, struct FONT *, const char *, int x, int y, int color);

void textprintf_trans_magic (
    struct BITMAP *, struct FONT *, int x, int y, int col, const char *, ...);
